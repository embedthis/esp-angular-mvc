{
    "name": "esp-angular-skeleton",
    "description": "ESP Angular MVC Application Skeleton",
    "version": "5.3.0",
    "licenses": [
        {
            "type": "GPL",
            "url": "http://www.gnu.org/licenses/gpl-2.0.html"
        }, {
            "type": "Commercial",
            "url": "https://embedthis.com/licensing/"
        }
    ],
    "keywords": [
        "esp"
    ],
    "dependencies": {
        "exp-ng": "~0.1",
        "exp-js": "~0.1",
        "exp-less": "~0.1",
        "exp-css": "~0.1",
        "exp-esp": "~0.1",
        "exp-gzip": "~0.1",
        "esp-angular": "~5.3",
        "esp-mvc": "~5.3"
    },
    "export": [
        {
            "from": [
                "documents/**",
                "layouts/**",
                "partials/**"
            ],
            "to": "${TOP}",
            "overwrite": false
        }
    ],
    "app": {
        "esp-angular-skeleton": {
            "js": [
                "app/main.js.ng",
                "app/*/**.js.ng",
                "app/*/**.js"
            ]
        },
    },
    "blend": [
        "esp.json",
        "expansive.json"
    ],
    "ignore": [
        "start.me"
    ]
}
