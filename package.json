{
    "name": "esp-angular-skeleton",
    "description": "ESP Angular MVC Application Skeleton",
    "version": "5.3.0",
    "licenses": [
        {
            "type": "GPL",
            "url": "http://www.gnu.org/licenses/gpl-2.0.html"
        }, {
            "type": "Commercial",
            "url": "https://embedthis.com/licensing/"
        }
    ],
    "keywords": [
        "esp"
    ],
    "dependencies": {
        "exp-ng": "~0.1",
        "exp-js": "~0.2",
        "exp-less": "~0.2",
        "exp-css": "~0.2",
        "exp-esp": "~0.1",
        "esp-angular": "~5.3",
        "esp-mvc": "~5.3"
    },
    "export": [
        {
            "from": [
                "layouts/**",
                "partials/**",
                "source/**"
            ],
            "to": "${TOP}",
            "overwrite": false
        }
    ],
    "render": {
        "js": [
            "${TOP}/${SOURCE}/main.js",
            "${TOP}/${SOURCE}/**.js.ng",
            "${TOP}/${SOURCE}/**.js",
            "!${TOP}/${SOURCE}/lib/**.js"
        ]
    },
    "blend": [
        "esp.json",
        "expansive.json"
    ],
    "ignore": [
        "start.me"
    ],
    "mode": "debug"
}
